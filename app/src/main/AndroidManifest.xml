<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.orderapp_085">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/nyangtto"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/nyangtto_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OrderApp_085"
        android:usesCleartextTraffic="true">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewStoreDetailActivity" />
        <activity android:name=".NameChangeActivity" />
        <activity android:name=".MainActivity">

        </activity>
    </application>

</manifest>